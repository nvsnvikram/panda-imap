#!/usr/bin/expect -f
set force_conservative 0
set timeout -1
spawn ../src/imapd
match_max 100000
expect -re "^\\* PREAUTH \\\[CAPABILITY IMAP4REV1 I18NLEVEL=1 LITERAL\\+ IDLE UIDPLUS NAMESPACE CHILDREN MAILBOX-REFERRALS BINARY UNSELECT WITHIN SORT THREAD=REFERENCES THREAD=ORDEREDSUBJECT MULTIAPPEND SCAN] Pre-authenticated user .+ .+ Panda IMAP 2018\.421 at "
send -- "001 CAPABILITY\r"
expect -exact "001 CAPABILITY\r
* CAPABILITY IMAP4REV1 I18NLEVEL=1 LITERAL+ IDLE UIDPLUS NAMESPACE CHILDREN MAILBOX-REFERRALS BINARY UNSELECT WITHIN SORT THREAD=REFERENCES THREAD=ORDEREDSUBJECT MULTIAPPEND SCAN SASL-IR LOGIN-REFERRALS STARTTLS LOGINDISABLED\r\r
001 OK CAPABILITY completed\r\r
"
send -- "002 LOGOUT\r"
expect -re "\\* BYE .+ IMAP4rev1 server terminating connection\r\r
002 OK LOGOUT completed\r\r"
expect eof
